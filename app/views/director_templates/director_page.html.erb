<h1>Director #<%= @director_id %> details</h1>
<a href="/directors" class="btn btn-block btn-outline-secondary">Go back</a>

<hr>

<dl>
  <dt>Name</dt>
  <dd><%= @name %></dd>

  <dt>Dob</dt>
  <dd><%= @dob %></dd>

  <dt>Bio</dt>
  <dd><%= @bio %></dd>

  <dt>Image</dt>
  <dd><img src="http://robohash.org/<%= @director_id %>">

  <dt>Created at</dt>
  <dd><%= @created %></dd>

  <dt>Updated at</dt>
  <dd><%= @updated %></dd>
</dl>